<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.SalesOrdersEntity">
        <table name="sales_orders" schema="public" catalog="Web"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="orderDate">
                <column name="order_date" precision="-1"/>
            </basic>
            <basic name="total">
                <column name="total" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
