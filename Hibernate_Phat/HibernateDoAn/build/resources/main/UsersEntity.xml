<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.UsersEntity">
        <table name="users" schema="public" catalog="Web"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="email">
                <column name="email"/>
            </basic>
            <basic name="firstName">
                <column name="first_name" nullable="false"/>
            </basic>
            <basic name="lastName">
                <column name="last_name" nullable="false"/>
            </basic>
            <basic name="active">
                <column name="active" nullable="false" precision="-1"/>
            </basic>
            <basic name="accId">
                <column name="acc_id" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
